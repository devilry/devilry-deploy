[buildout]
extends = https://raw.github.com/devilry/devilry-deploy/<%= @devilry_version %>/buildout/buildout-base.cfg

[supervisor]
<% @supervisor.each_pair do |name, value| -%>
<%= "#{name} = #{value}" %>
<% end -%>
pidfile = <%= @pidfile %>
supervisord-user = <%= @username %>

[gunicorn]
<% @gunicorn.each_pair do |name, value| -%>
<%= "#{name} = #{value}" %>
<% end -%>
